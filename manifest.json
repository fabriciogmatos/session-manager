{
	"manifest_version": 3,
	"name": "Session Manager Extended 2",
	"version": "1.0.0",
	"description": "Simple yet powerful tab set management. Quickly and easily save, update, remove, restore sets of tabs and search now!",
	"icons": {
		"16": "img/16.png",
		"128": "img/128.png"
	},
	"action": {
		"default_icon": {
			"16": "img/16.png",
			"48": "img/48.png"
		},
		"default_title": "Session Manager Extended 2",
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "js/background.js"
	},
	"permissions": ["tabs", "storage"],
	"host_permissions": ["<all_urls>"],
	"web_accessible_resources": [
		{
			"resources": ["img/*"],
			"matches": ["<all_urls>"]
		}
	],
	"content_security_policy": {
		"content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
	},
	"options_ui": {
		"page": "options.html"
	},
	"omnibox": {
		"keyword": "session"
	}
}
